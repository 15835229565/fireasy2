{
  "fireasy": {
    "loggings": {
      "settings": {
        "log4net": {
          "type": "Fireasy.Log4net.Logger,Fireasy.Log4net"
        },
        "nlog": {
          "type": "Fireasy.NLog.Logger,Fireasy.NLog"
        }
      }
    },
    "subscribers": {
      "settings": {
        "redis": {
          "type": "Fireasy.Redis.RedisSubscribeManager, Fireasy.Redis",
          "config": {
            "defaultDb": 11,
            "connectTimeout": 5,
            "host": [
              {
                "server": "localhost"
              }
            ]
          }
        }
      }
    },
    "cachings": {
      "settings": {
        "redis": {
          "type": "Fireasy.Redis.CacheManager, Fireasy.Redis",
          "config": {
            "defaultDb": 10,
            "connectTimeout": 5,
            "host": [
              {
                "server": "localhost"
              }
            ]
          }
        }
      }
    }
  }
}
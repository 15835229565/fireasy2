{
  "fireasy": {
    "loggings": {
      "level": "Debug;Info"
    },
    "subscribers": {
      "settings": {
        "redis": {
          "type": "Fireasy.Redis.SubscribeManager, Fireasy.Redis",
          "config": {
            "defaultDb": 11,
            "host": [
              {
                "server": "127.0.0.1"
              }
            ]
          }
        },
        "rabbit": {
          "type": "Fireasy.RabbitMQ.SubscribeManager, Fireasy.RabbitMQ",
          "config": {
            "userName": "guest",
            "password": "123",
            "server": "amqp://127.0.0.1:5672"
          }
        }
      }
    },
    "cachings": {
      "settings": {
        "redis": {
          "type": "Fireasy.Redis.CacheManager, Fireasy.Redis",
          "config": {
            "defaultDb": 10,
            "connectTimeout": 5,
            "poolSize": 10,
            "host": [
              {
                "server": "127.0.0.1"
              }
            ]
          }
        }
      }
    },
    "imports": {
      "settings": [
        { "assembly": "Fireasy.Common.Tests" }
      ]
    }
  }
}